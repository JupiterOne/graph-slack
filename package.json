{
  "name": "@jupiterone/graph-slack",
  "version": "2.3.0",
  "description": "A graph conversion tool for https://slack.com",
  "license": "MPL-2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "j1-integration collect",
    "graph": "j1-integration visualize",
    "lint": "eslint . --cache --fix --ext .ts,.tsx",
    "format": "prettier --write '**/*.{ts,js,json,css,md,yml}'",
    "type-check": "tsc",
    "test": "jest",
    "test:ci": "yarn lint && yarn type-check && yarn test",
    "prebuild": "yarn test:ci",
    "prepush": "yarn test:ci",
    "build": "tsc -p tsconfig.dist.json --declaration",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@lifeomic/attempt": "^3.0.0",
    "@slack/web-api": "^5.10.0"
  },
  "devDependencies": {
    "@jupiterone/integration-sdk-core": "^2.3.0",
    "@jupiterone/integration-sdk-dev-tools": "^2.3.0",
    "@jupiterone/integration-sdk-testing": "^2.3.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.11.1",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "type-fest": "^0.13.1",
    "typescript": "^3.8.3",
    "uuid": "^8.2.0"
  },
  "peerDependencies": {
    "@jupiterone/integration-sdk-core": "^2.3.0"
  }
}
